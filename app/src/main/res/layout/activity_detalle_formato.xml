<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Formatos.DetalleFormatoActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp">


            <ImageView
                android:id="@+id/img3D"
                android:layout_width="210dp"
                android:layout_height="100dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/f_1" />

            <ImageView
                android:id="@+id/imgDBOX"
                android:layout_width="210dp"
                android:layout_height="100dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/f_2" />

        </LinearLayout>

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/fragmentContentFormatos"
            android:name="idat.dami.chinestarapp.Formatos.Formatos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>